			if require(script)[Args[1]] then
				Output("`" .. Args[1] .. "`" .. " " .. require(script)[Args[1]]["description"])
			else
				Output("Commands are listed below. To learn more about them, do `help` <command>.")
				for k, v in pairs(require(script)) do
					Output(k)
				end
			end
		end,
		description = "shows help for a given command, or all of them if no command was provided.",
		arguments = {
			["command"] = CommandArgumentType.Optional,
		},
	},
	whoami = {
		command = function(_, Output)
			local User = getfenv(0)["Player"]
			local JoinDate = os.date("!*t", os.time() - (User.AccountAge * 86400))
			local OnlineFriends = #User:GetFriendsOnline()
			Output([[Showing user information for #]] .. User.UserId)
			if User.Name == User.DisplayName then
				Output([[You're]] .. " " .. User.Name .. [[.]])
			else
				Output([[You're]] .. " " .. User.Name .. " " .. [[("]] .. User.DisplayName .. [[").]])
			end
			Output([[You've been on Roblox since]] .. " " .. JoinDate.day .. [[/]] .. JoinDate.month .. [[/]] .. JoinDate.year .. [[. (D/M/Y)]])
			Output([[You have]] .. " " .. tostring(OnlineFriends) .. " " .. [[online friends.]])
		end,
		description = "outputs some information about you, the player.",
	},
	clear = {
		command = function(_, _)
			for _, v in ipairs(getfenv(0)["GUI"]:GetChildren()) do
				if v.Name == "Line" then
					v:Destroy()
				end
			end
		end,
		description = "clears all lines on the screen.",
	},
	cd = {
		command = function(Args, _)
			if (Args[1] == ".." or Args[1] == ".Parent") and getfenv(0)["Directory"].Parent then
				script.cd:FireServer(getfenv(0)["Directory"].Parent)
			elseif Args[1] and getfenv(0)["Directory"]:FindFirstChild(Args[1]) then
				script.cd:FireServer(getfenv(0)["Directory"]:FindFirstChild(Args[1]))
			elseif string.sub(Args[1], 1, 4) == "game" then
				script.cd:FireServer(require(game:GetService("ReplicatedStorage"):WaitForChild("Loadstring", 0.5))("return " .. Args[1])())
