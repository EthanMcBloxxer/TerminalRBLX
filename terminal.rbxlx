					<ProtectedString name="Source"><![CDATA[local extension = {
	extension = "rQuery"
}

local function init(player)
	if player and player:IsA("Player") then
		if pcall(function() assert(player.PlayerGui.Drawing) end) then
			return player.PlayerGui.Drawing
		else
			local gui = Instance.new("ScreenGui")
			gui.Name = "Drawing"
			gui.Parent = player.PlayerGui
			return gui
		end
	end
end

function extension.square(player, size, position, color) -- implement `filled` when UIStroke is ready
		local element = Instance.new("Frame")
		element.BorderSizePixel = 0
		element.Name = "UnnamedDrawing"
		if typeof(size) == "Vector2" then
			size = UDim2.fromOffset(size.X, size.Y)
		end
		element.Size = size
		if typeof(position) == "Vector2" then
			position = UDim2.fromOffset(position.X, position.Y)
		end
		element.Position = position
		element.BackgroundColor3 = color
		element.Parent = init(player)
		return element
end

function extension.circle(player, size, position, radius, color) -- implement `filled` when UIStroke is ready
	size = UDim2.fromOffset(size, size)
	local element = extension.square(player, size, position, color)
	local rounder = Instance.new("UICorner")
	if radius <= 0.5 then
		rounder.CornerRadius = UDim.new(radius, 0)
	else
		rounder.CornerRadius = UDim.new(0, radius)
	end
	rounder.Parent = element
	return element
end

function extension.text(player, text, size, position, center, color, outline, font)
	local element = Instance.new("TextLabel")
	element.AutomaticSize = Enum.AutomaticSize.XY
	element.BackgroundTransparency = 1
	element.Name = text
	element.Text = text
	element.TextSize = size
	element.TextColor3 = color
	element.Font = font or Enum.Font[font]
	if typeof(position) == "Vector2" then
		position = UDim2.fromOffset(position.X, position.Y)
	end
	element.Position = position
	if outline then
		element.TextStrokeTransparency = 0
		element.TextStrokeColor3 = outline
	end
	element.Parent = init(player)
	if center then
		element.Position = UDim2.fromOffset(element.AbsolutePosition.X - (element.TextBounds.X / 2), element.AbsolutePosition.Y - (element.TextBounds.Y / 2))
	end
	return element
end


function extension.image(player, id, size, position, rounding, crop)
	if type(id) == "number" then
		id = "rbxassetid://" .. id
	end
	local element = Instance.new("ImageLabel")
	element.BackgroundTransparency = 1
	element.Name = "UnnamedDrawing"
	if crop then
		element.ScaleType = Enum.ScaleType.Crop
	else
		element.ScaleType = Enum.ScaleType.Fit
	end
	element.Image = id
	if typeof(size) == "Vector2" then
		size = UDim2.fromOffset(size.X, size.Y)
	end
	element.Size = size
	if typeof(position) == "Vector2" then
		position = UDim2.fromOffset(position.X, position.Y)
	end
	element.Position = position
	element.Parent = init(player)
	if rounding then
		local rounder = Instance.new("UICorner")
		if rounding <= 0.5 then
			rounder.CornerRadius = UDim.new(rounding, 0)
		else
			rounder.CornerRadius = UDim.new(0, rounding)
		end
		rounder.Parent = element
	end
	return element
end

return extension
