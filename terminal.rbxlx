	if not Paired then
		local Rounded = math.floor(Input)
		Input = tostring(Rounded)
	end

	if Negative then
		return "-" .. Input
	end
	return Input
end

function exports.httpraw(options)
	local response
	local success, message = pcall(function()
		response = game:GetService("HttpService"):RequestAsync(options)
	end)
	if success then
		if response.Success then
			return response
		else
			error(response.StatusMessage)
			return false
		end
