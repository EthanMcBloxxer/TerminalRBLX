		b = z(i, b, c, d, a, X[9], 21, t[64])

		return A + a, B + b, C + c, D + d
	end
	local function leIstr(i)
		local f = function(s)
			return string.char(bit_and(bit_rshift(i, s), 255))
		end
		return f(0) .. f(8) .. f(16) .. f(24)
	end
	local function beInt(s)
		local v = 0
		for i = 1, string.len(s) do
			v = v * 256 + string.byte(s, i)
		end
		return v
	end
	local function leInt(s)
		local v = 0
		for i = string.len(s), 1, -1 do
			v = v * 256 + string.byte(s, i)
		end
		return v
	end
	local function leStrCuts(s, ...)
		local arg = { ... }
		local o, r = 1, {}
		for i = 1, #arg do
			table.insert(r, leInt(string.sub(s, o, o + arg[i] - 1)))
			o = o + arg[i]
		end
		return r
	end

	local msgLen = string.len(message)
	local padLen = 56 - msgLen % 64
	if msgLen % 64 > 56 then
		padLen = padLen + 64
	end
	if padLen == 0 then
		padLen = 64
	end
	message = message .. string.char(128) .. string.rep(string.char(0), padLen - 1)
	message = message .. leIstr(8 * msgLen) .. leIstr(0)
	assert(string.len(message) % 64 == 0)
	local t = md5.consts
	local a, b, c, d = t[65], t[66], t[67], t[68]
	for i = 1, string.len(message), 64 do
		local X = leStrCuts(string.sub(message, i, i + 63), 4, 4, 4, 4, 4, 4, 4, 4, 4, 4, 4, 4, 4, 4, 4, 4)
		assert(#X == 16)
		X[0] = table.remove(X, 1)
		a, b, c, d = md5.transform(a, b, c, d, X)
	end
	local swap = function(w)
		return beInt(leIstr(w))
	end
	return string.format("%08x%08x%08x%08x", swap(a), swap(b), swap(c), swap(d))
end

