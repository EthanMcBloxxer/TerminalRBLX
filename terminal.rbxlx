end

function exports.play(volume, loop, ...)
	if not volume then volume = 1 end
	local sounds = {...}
	local function run(sounds)
		for _, v in ipairs(sounds) do
			local sound = Instance.new("Sound")
			sound.Volume = volume
			sound.SoundId = "rbxassetid://"..v
			sound.Parent = workspace
			sound:Play()
			sound.Ended:Wait()
			wait(1)
			sound:Destroy()
		end
	end
	coroutine.resume(coroutine.create(function()
		if loop == true then
			while true do
				run(sounds)
			end
		else
			run(sounds)
		end
	end))
end

function exports.daynightcycle(day, night)
	local startTime = tick()
	if not day then
		day = 10
	end
	if not night then
		night = 5
	end
	coroutine.resume(coroutine.create(function()
		while true do
			local currentTime = (tick() - startTime) / 60 % (day + day)
			if currentTime < day then
				game.Lighting.ClockTime = 6 + 12 * currentTime / day
			else
				game.Lighting.ClockTime = (18 + 12 * (currentTime - day) / day) % 24
			end
			wait()
		end
	end))
end

function exports.fps(sample)
	local fps = {}
	local connection = game:GetService("RunService").Stepped:Connect(function(_, t)
		fps[table.getn(fps) + 1] = math.ceil(1 / t)
	end)
	wait(sample)
	connection:Disconnect()
	local avg = 0
	for _, v in ipairs(fps) do
		avg += v
	end
	return math.floor(avg / #fps)
end

function exports.teleport(place, player, gui)
	if gui then
