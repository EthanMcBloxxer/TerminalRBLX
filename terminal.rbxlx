function exports.reverse(data)
	if type(data) == "string" then
		return string.reverse(data)
	elseif type(data) == "number" then
		return tonumber(string.reverse(tostring(data)))
	elseif type(data) == "boolean" then
		return not data
	elseif type(data) == "table" then
		for i = 1, math.floor(#data / 2) do
			data[i], data[#data - i + 1] = data[#data - i + 1], data[i]
		end
		return data
	else
		error("[rQuery] Unsupported datatype: " .. typeof(data) .. ".")
	end
end

function exports.append(base, appendant)
