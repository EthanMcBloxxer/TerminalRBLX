					<ProtectedString name="Source"><![CDATA[--// Declarations //--

local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Commands = require(ReplicatedStorage:WaitForChild("Commands"))

Player = game:GetService("Players").LocalPlayer
GUI = Player:WaitForChild("PlayerGui"):WaitForChild("Terminal"):WaitForChild("Container")

Directory = game
LineCount = 0
Colors = {
	Foreground = {
		Default = Color3.fromRGB(0, 202, 0),
		Warning = Color3.fromRGB(255, 170, 0),
		Error = Color3.fromRGB(202, 0, 0),
	},
	Background = Color3.fromRGB(34, 34, 34),
}

function Parse(Input)
	Input = string.gsub(Input, "[\t\n%c]", "")
	if Input ~= "" and string.find(Input, "%s+$") then
		Input = string.sub(Input, 1, string.find(Input, "%s+$") - 1)
	end
	local All = string.split(Input, " ")
	local Command = string.lower(All[1])
	table.remove(All, 1) 
	return Command, All
end
function Output(Text, Color)
	if not Text then
		Text = ""
	end
	if not Color then
		Color = Colors.Foreground.Default
	end
	
	LineCount += 1
	
	local Line = ReplicatedStorage.Line:Clone()
	Line.LayoutOrder = LineCount
	Line.Text = Text
	Line.TextColor3 = Color
	Line.Parent = GUI
end

--// Pre-load Setup //--

game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.Backpack, true)

Output(string.format("Roblox Terminal [%s]", version()))
Output()

GUI.Input.Box:CaptureFocus()

--// Events //--

GUI.Input.Box.FocusLost:Connect(function()
	local Command, Args = Parse(GUI.Input.Box.Text)
	if Commands[Command] then
		Commands[Command].command(Args, Output)
	else
		local Executable, Reason = require(ReplicatedStorage:WaitForChild("Loadstring", 1))(GUI.Input.Box.Text)
		if not Reason then
			if string.gsub(GUI.Input.Box.Text, "[\t\n%c]", "") == "print(getfenv(0))" then
				Output("github.com/EthanMcBloxxer/TerminalRBLX ;)")
			end
			Executable()
		end
	end
	GUI.Input.Box:CaptureFocus()
end)

ReplicatedStorage:WaitForChild("Commands"):WaitForChild("cd").OnClientEvent:Connect(function(NewDirectory)
	if NewDirectory then
		Directory = NewDirectory
		if Directory == game or NewDirectory == game then
			GUI.Input.Line.Text = "game"
		else
			GUI.Input.Line.Text = "game." .. Directory:GetFullName()
		end
	end
end)

game:GetService("LogService").MessageOut:Connect(function(Message, Kind)
	Message = tostring(Message)
	if Kind == Enum.MessageType.MessageOutput or Kind == Enum.MessageType.MessageInfo then
		Output(Message, Colors.Foreground.Default)
	elseif Kind == Enum.MessageType.MessageWarning then
		Output(Message, Colors.Foreground.Warning)
	elseif Kind == Enum.MessageType.MessageError then
		Output(Message, Colors.Foreground.Error)
	else
		Output(Message)
	end
end)
]]></ProtectedString>
