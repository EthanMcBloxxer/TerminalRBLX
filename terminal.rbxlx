function exports.tween(instance, goals, seconds, style, direction, repeats, reverse)
	if not seconds then seconds = 2.5 end
	if not style then style = "Linear" end
	if not direction then direction = "InOut" end
	if not repeats then repeats = 0 end
	if not reverse then reverse = false end
	if instance and goals then
		game:GetService("TweenService"):Create(instance, TweenInfo.new(seconds, Enum.EasingStyle[style], Enum.EasingDirection[direction], repeats, reverse, 0), goals):Play()
	end
	return instance
end

function exports.notify(message, title, icon, duration, button1, button2, callback)
	if not title then
