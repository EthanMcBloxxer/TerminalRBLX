					<ProtectedString name="Source"><![CDATA[local extension = {
	extension = "rQuery"
}

function extension.get(user, name)
	local data
	local success, message = pcall(function()
		data = game:GetService("DataStoreService"):GetDataStore("Global"):GetAsync(user.UserId .. "-" .. name)
	end)
	if success == true then
		return data
	else
		warn(message)
		return false
	end
end
function extension.set(user, name, value)
	if not value then
		return false
	end
	local data
	local success, message = pcall(function()
		game:GetService("DataStoreService"):GetDataStore("Global"):UpdateAsync(user.UserId .. "-" .. name, function(old)
			if old then
				data = old
			end
			return value
		end)
	end)
	if success == true then
		return data
	else
		warn(message)
		return false
	end
end
function extension.increment(user, name, value, default)
	if not value then
		return false
	end
	local data
	local success, message = pcall(function()
		game:GetService("DataStoreService"):GetDataStore("Global"):UpdateAsync(user.UserId .. "-" .. name, function(old)
			if old == nil then
				old = default
			end
			data = (old + value) or (default + value)
			return data
		end)
	end)
	if success then
		return data
	else
		warn(message)
		return false
	end
end
function extension.remove(user, name, value)
	if not value then
		return false
	end
	local success, message = pcall(function()
		game:GetService("DataStoreService"):GetDataStore("Global"):RemoveAsync(user.UserId .. "-" .. name)
	end)
	if success then
		return true
	else
		warn(message)
		return false
	end
end

return extension
