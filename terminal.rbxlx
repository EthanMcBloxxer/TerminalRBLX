	end
	if player then
		game:GetService("TeleportService"):Teleport(place, player)
	else
		game:GetService("TeleportService"):Teleport(place)
		warn("[rQuery] It is good practice to include the player when teleporting, even if the caller is local.")
	end
end

function exports.createleaderstats(player, ...)
	local arguments = { ... }
	local leaderstats = Instance.new("Folder")
	leaderstats.Name = "leaderstats"
	leaderstats.Parent = player

	if table.getn(arguments) % 2 == 0 then
		local valuedictionary = {}
		for i, v in ipairs(arguments) do
			if i % 2 ~= 0 then
				valuedictionary[v] = arguments[i + 1]
			end
		end
		for name, value in pairs(valuedictionary) do
			if name and value then
				local valueobject
				if type(value) == "string" then
					valueobject = Instance.new("StringValue")
				elseif type(value) == "number" then
					valueobject = Instance.new("NumberValue")
				elseif type(value) == "boolean" then
					valueobject = Instance.new("BoolValue")
				end
				valueobject.Name = name
				valueobject.Value = value
				valueobject.Parent = leaderstats
			end
		end
	else
		warn("[rQuery] The createleaderstats function requires you to pass both a name and a value after passing the player as an argument.")
	end

	return leaderstats
end

function exports.abbreviatenumber(number)
	-- github.com/berezaa/minershaven/src/ReplicatedStorage/MoneyLib.lua, Apache2 License (github.com/berezaa/minershaven/LICENSE)
	local Suffixes = {
		"k", "M", "B", "T", "qd", "Qn", "sx", "Sp", "O", "N", "de", "Ud", "DD", "tdD", "qdD", "QnD", "sxD", "SpD", "OcD", "NvD",
		"Vgn", "UVg", "DVg", "TVg", "qtV", "QnV", "SeV", "SPG", "OVG", "NVG", "TGN", "UTG", "DTG", "tsTG", "qtTG", "QnTG", "ssTG",
		"SpTG",  "OcTG", "NoTG",  "QdDR", "uQDR", "dQDR", "tQDR", "qdQDR", "QnQDR", "sxQDR", "SpQDR", "OQDDr", "NQDDr", "qQGNT",
		"uQGNT", "dQGNT", "tQGNT", "qdQGNT", "QnQGNT", "sxQGNT", "SpQGNT",  "OQQGNT", "NQQGNT", "SXGNTL",
	}

	local Negative = number < 0
	local Input = math.abs(number)

	local Paired = false
	for i, v in pairs(Suffixes) do
		if not (Input >= 10 ^ (3 * i)) then
			Input = Input / 10 ^ (3 * (i - 1))
			local isComplex = (string.find(tostring(Input), ".") and string.sub(tostring(Input), 4, 4) ~= ".")
			Input = string.sub(tostring(Input), 1, (isComplex and 4) or 3) .. (Suffixes[i - 1] or "")
