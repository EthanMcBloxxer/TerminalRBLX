function exports.httpget(url, json)
	local Service = game:GetService("HttpService")
	if json then
		local response, data
		pcall(function()
			response = Service:GetAsync(url)
			data = Service:JSONDecode(response)
		end)
		if data then
			return data
		else
			warn("[rQuery] The HTTPRequest or JSONDecode failed.")
			return false
		end
	else
		local response
		pcall(function()
			response = Service:GetAsync(url)
		end)
		if response then
			return response
		else
			warn("[rQuery] The HTTPRequest failed.")
			return false
		end
	end
end
function exports.discord(player, message, username, webhook, max)
	local Service = game:GetService("HttpService")
	if not username then
		username = player.Name
	end
	if not max then
		max = math.huge
	end
	if max - #message >= 0 and message ~= "" then
		local data = {
			["content"] = game:GetService("Chat"):FilterStringForBroadcast(message, player),
			["username"] = username,
		}
		return Service:PostAsync(webhook, Service:JSONEncode(data))
	end
end

